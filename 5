#include<stdio.h>
int main()
{
    char str[100];
    printf("Input: ");
    gets(str);
    
    int i = 0;
    while(str[i]!='\0')
    {
        if(str[i]==' ')
        {
            i++;
            continue;
        }
        
        else if((str[i]>='a' && str[i]<='z') || (str[i]>='A' && str[i]<='Z'))
        {
            printf("IDENTIFIER: %c\n",str[i]);
            i++;
        }
        
        else if(str[i]>='0' && str[i]<='9')
        {
            printf("NUMBER: ");
            while(str[i]>='0' && str[i]<='9')
            {
                printf("%c",str[i]);
                i++;
            }
            printf("\n");
        }
        
        else if(str[i]=='+' || str[i]=='-' || str[i]=='*' || str[i]=='/')
        {
            printf("OPERATOR: %c\n",str[i]);
            i++;
        }
        
        else if(str[i]=='(' || str[i]==')')
        {
            printf("PARENTHESIS: %c\n",str[i]);
            i++;
        }
        
        else
        {
            printf("Unknown %c\n", str[i]);
            i++;
        }
        
    }
    
    return 0;
}
