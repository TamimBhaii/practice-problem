#include <stdio.h>

int main()
{
    char str[100];

    while (scanf("%[^\n]", str) != EOF)
    {
        getchar(); // consume newline after scanf

        if ((str[0] == '+' || str[0] == '-' || str[0] == '*' || str[0] == '/' || str[0] == '%') && str[1] == '\0')
        {
            printf("'%s': Arithmetic Operator\n", str);
        }
        else if ((str[0] == '=' && str[1] == '=' && str[2] == '\0') ||
                 (str[0] == '!' && str[1] == '=' && str[2] == '\0') ||
                 (str[0] == '<' && str[1] == '=' && str[2] == '\0') ||
                 (str[0] == '>' && str[1] == '=' && str[2] == '\0') ||
                 (str[0] == '<' && str[1] == '\0') ||
                 (str[0] == '>' && str[1] == '\0'))
        {
            printf("'%s': Relational Operator\n", str);
        }
        else if ((str[0] == '&' && str[1] == '&' && str[2] == '\0') ||
                 (str[0] == '|' && str[1] == '|' && str[2] == '\0') ||
                 (str[0] == '!' && str[1] == '\0'))
        {
            printf("'%s': Logical Operator\n", str);
        }
        else if (str[0] == '=' && str[1] == '\0')
        {
            printf("'%s': Assignment Operator\n", str);
        }
        else
        {
            printf("'%s': Invalid Operator\n", str);
        }
    }

    return 0;
}
