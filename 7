#include <stdio.h>

int main()
{
    char str[100];
    
    int len = 0, b=0, valid=1;
    printf("Enter a string to check against the regex: ");
    gets(str);
    
    while(str[len]!='\0')
    {
        len++;
        
    }
    
    for(int i = 0; i<len; i++)
    {
        if(str[i]!='a' && str[i]!='b')
        {
            valid=0;
            break;
            
        }
        if(str[i]=='b')
        {
            b++;
        }
    }
    if(b<2)
    {
        valid=0;
    }
    if(valid)
    {
        printf("'%s' MATCHES the regular expression.\n", str);
    }
    else
    {
        printf("'%s' DOES NOT MATCHES the regular expression.\n", str);
    }
    


    return 0;
}
