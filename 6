#include <stdio.h>

int main() {
    char str[100];
    int i, valid = 1;

    printf("Enter a string to check against regex 'a(a|b)*a': ");
    scanf("%s", str);

    if (str[0] != 'a') valid = 0;
    else {
        for (i = 1; str[i+1] != '\0'; i++) {
            if (!(str[i] == 'a' || str[i] == 'b')) {
                valid = 0;
                break;
            }
        }
        if (str[i] != 'a') valid = 0;
    }

    if (valid)
        printf("'%s' MATCHES the regular expression 'a(a|b)*a'.\n", str);
    else
        printf("'%s' DOES NOT MATCH the regular expression 'a(a|b)*a'.\n", str);

    return 0;
}
